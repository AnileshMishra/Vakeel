package com.vakeel.master.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;

import com.vakeel.master.modal.StateMaster;

public interface JpaStateMasterRepository extends JpaRepository<StateMaster, Long> {
	
	List<StateMaster> findAllByIsDeleted(boolean isDeleted);
	
	StateMaster findByStateCodeAndIsDeleted(String stateCode);
}
